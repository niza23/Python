    "m=n\n",
    "sum=0\n",
    "while n!=0:\n",
    "    x=n%10\n",
    "    y=x*x*x\n",
    "    sum=sum+y\n",
    "    n=n//10\n",
    "    \n",
    "if(m==sum):\n",
    "    print(\"It is Armstrong Number\")\n",
    "else:\n",
    "    print(\"It is Not An Armstrong Number \")"
