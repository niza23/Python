   "source": [
    "n=int(input(\"Enter Number To Be Reversed \"))\n",
    "print(\"Before Reverse Your Number Is : %d\" %n)\n",
    "reverse=0\n",
    "while n!=0:\n",
    "    reverse=reverse*10 + n%10\n",
    "    n=n//10\n",
    "    \n",
    "print(\"After Reverse : %d\" %reverse)\n"
