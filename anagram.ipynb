    "def anagramCheck(str1,str2):\n",
    "    if(sorted(str1)==sorted(str2)):\n",
    "        return 1\n",
    "    else:\n",
    "        return 2\n",
    "    \n",
    "str1=input(\"Enter 1st Number \")\n",
    "str2=input(\"Enter 2nd Number \")\n",
    "if anagramCheck(str1,str2):\n",
    "    print(\"Anagram\")\n",
    "else:\n",
    "    print(\"Not a Anagram\")"
   ]
