   "source": [
    "s=input(\"Enter A The String is Palindrome \")\n",
    "a=len(s)\n",
    "f=0\n",
    "\n",
    "for i in range(a):\n",
    "    if(s[i]==s[a-i-1]):\n",
    "        f=1\n",
    "    else:\n",
    "        f=0\n",
    "        break\n",
    "    \n",
    "if(f==1):\n",
    "    print(\"Its Palindrome\")\n",
    "else:\n",
    "    print(\"Its not  Palindrome\")"
   ]
  }
